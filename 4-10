foods = ['cake', 'pizza', 'ice cream', 'chips']

print("The first three items on the list are:")
for food in foods[:3]:
	print(food.title())


print("\nThree items from the middle of the list are:")
for food in foods[1:3]:
	print(food.title())

print("\nThe last three items on the list are:")
for food in foods[1:]:
	print(food.title())
